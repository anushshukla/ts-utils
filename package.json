{
  "name": "hybrid-integration-engine",
  "//name": "project / application name",
  "version": "1.0.0",
  "//version": "MAJOR.MINOR.PATCH | Please refer https://semver.org/",
  "description": "Provide qualified data to HDE",
  "main": "build/src/index.js",
  "scripts": {
    "install:ci": "npm  ci --prefer-offline --no-audit",
    "install:prod": "npm  ci --cache .npm --production --prefer-offline --no-audit",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:dev": "tsc --project tsconfig.dev.json",
    "start:server": "docker composer up",
    "test": "jest --detectOpenHandles --coverage --verbose",
    "test:coverage": "jest --detectOpenHandles --noStackTrace --silent --bail --coverage",
    "test:code:quality": "sonarlint --src 'src/**' --tests 'test/**'",
    "test:coverage:ci": "npm run test:coverage --silent --noStackTrace --ci --bail --json",
    "lint": "eslint src",
    "lint:changes": "func() { eslint $(git diff --name-only --diff-filter=ACMRTUXB origin/$1 | grep  -E \"(.js$|.ts$|.tsx$)\"); }; func",
    "lint:fix": "eslint src --fix",
    "clean": "rimraf ./build",
    "start:api:dev": "NODE_ENV=local ts-node-dev --watch src -r tsconfig-paths/register --transpile-only src/index.ts",
    "start:consumer:dev": "NODE_ENV=alpha ts-node-dev --watch src -r tsconfig-paths/register --transpile-only src/server/queue/consumers/index.ts",
    "test:watch": "jest --config tests/jest.getEnv(json) --detectOpenHandles --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hubilo/hybrid-integration-ms.git"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
  },
  "devDependencies": {
    "@types/lodash": "4.14.184",
    "@typescript-eslint/eslint-plugin": "5.36.0",
    "@typescript-eslint/parser": "5.36.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "commitlint": "13.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-html-reporter": "0.7.4",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jsdoc": "3.6.10",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "sonarlint": "1.1.0",
    "ts-jest": "27.1.1",
    "ts-node": "10.1.0",
    "ts-node-dev": "1.1.8"
  },
  "engines": {
    "npm": "v8.15.0",
    "node": "v16.17.0"
  }
}
